    // ---------------------------------------------------------------------------
    // Find and Flash by ID
    // ---------------------------------------------------------------------------
    function getInputValue() {
        return document.getElementById('input_centerloc').value.trim();
    }

    function featureMatches(f, idType, val) {
        // handle numbers that came in as strings, etc.
        return String(f.properties[idType]) === String(val);
    }

    function findAndFlash(idType) {
        const val = getInputValue();
        const features = sub_ctrl.glLayer._shapes.features;

        const hit = features.find(f => featureMatches(f, idType, val));

        if (!hit) {
            console.warn(`No feature with ${idType} = ${val}`);
            return;
        }

        // Re-use your flash routine
        flashPolygon(
            L.geoJSON(hit, {
                style: {
                    color: "#ffffff",
                    weight: 3,
                    opacity: 1,
                    fillColor: "#ffffff",
                    fillOpacity: 1.0
                },
                pane: 'markerCustomPane'
            })
        );
    }

    document.getElementById('btn_find_topaz_id')
        .addEventListener('click', () => findAndFlash('TopazID'));

    document.getElementById('btn_find_wepp_id')
        .addEventListener('click', () => findAndFlash('WeppID'));

    function flashPolygon(layer) {
        layer.addTo(map);
        // map.fitBounds(layer.getBounds());     // optional: zoom to the hit
        setTimeout(() => map.removeLayer(layer), 1000);
    }


