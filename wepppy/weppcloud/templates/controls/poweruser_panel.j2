  }

  #puModal .modal-content {
    display: flex;
    flex-direction: column;
    max-height: calc(100vh - 144px);
  }

  #puModal .modal-body {
    overflow-y: auto;
  }

  @media (max-width: 768px) {
    #puModal .modal-dialog {
      margin: 72px auto 24px;
