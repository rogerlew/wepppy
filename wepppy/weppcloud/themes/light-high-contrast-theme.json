{
  "$schema": "vscode://schemas/color-theme",
  "name": "WEPPcloud Light High Contrast",
  "type": "light",
  "semanticHighlighting": true,
  "colors": {
    "activityBar.background": "#EFF4FF",
    "activityBar.foreground": "#0A1F33",
    "activityBar.inactiveForeground": "#4A5D73",
    "activityBar.activeBackground": "#D7E8FF",
    "activityBar.activeBorder": "#0050A4",
    "activityBar.dropBackground": "#FFB70360",
    "activityBarBadge.background": "#0050A4",
    "activityBarBadge.foreground": "#FFFFFF",
    "badge.background": "#C25A00",
    "badge.foreground": "#FFFFFF",
    "breadcrumb.background": "#F8FBFF",
    "breadcrumb.foreground": "#34475F",
    "button.background": "#0050A4",
    "button.foreground": "#FFFFFF",
    "button.hoverBackground": "#003C7A",
    "contrastBorder": "#002347",
    "contrastActiveBorder": "#FFB703",
    "dropdown.border": "#0F3A5F",
    "editor.background": "#FFFEFA",
    "editor.foreground": "#051526",
    "editor.selectionBackground": "#FFD54F80",
    "editor.selectionHighlightBackground": "#FFE38466",
    "editor.inactiveSelectionBackground": "#FFE3844D",
    "editor.lineHighlightBackground": "#F7F3E8",
    "editorCursor.foreground": "#001122",
    "editorLineNumber.foreground": "#004E89",
    "editorLineNumber.activeForeground": "#001632",
    "editorInlayHint.foreground": "#7A3EB1",
    "editorCodeLens.foreground": "#7A3EB1",
    "editorGutter.addedBackground": "#0F8B25",
    "editorGutter.modifiedBackground": "#0062B8",
    "editorGutter.deletedBackground": "#B00020",
    "editorGutter.commentRangeForeground": "#8A2BE2",
    "editorBracketMatch.background": "#FFB70333",
    "editorBracketMatch.border": "#FF6F00",
    "editorGroupHeader.tabsBackground": "#EFF4FF",
    "editorGroup.background": "#E2ECFF",
    "editorGroup.border": "#0F3A5F",
    "editorWidget.background": "#FFFFFF",
    "editorWidget.border": "#0F3A5F",
    "editorSuggestWidget.background": "#FFFFFF",
    "editorSuggestWidget.foreground": "#051526",
    "editorHoverWidget.background": "#FFFFFF",
    "editorHoverWidget.border": "#0F3A5F",
    "editorInfo.foreground": "#005FB8",
    "editorWarning.foreground": "#C25A00",
    "editorError.foreground": "#B00020",
    "editorOverviewRuler.addedForeground": "#0F8B25",
    "editorOverviewRuler.modifiedForeground": "#0062B8",
    "editorOverviewRuler.deletedForeground": "#B00020",
    "editorOverviewRuler.errorForeground": "#B00020",
    "editorOverviewRuler.warningForeground": "#C25A00",
    "editorOverviewRuler.currentContentForeground": "#0050A4",
    "editorOverviewRuler.commonContentForeground": "#0050A4",
    "editorOverviewRuler.border": "#BAC8DA",
    "extensionButton.prominentBackground": "#0050A4",
    "extensionButton.prominentForeground": "#FFFFFF",
    "extensionButton.prominentHoverBackground": "#003C7A",
    "focusBorder": "#0050A4",
    "foreground": "#051526",
    "input.background": "#FFFFFF",
    "input.foreground": "#051526",
    "input.border": "#0F3A5F",
    "input.placeholderForeground": "#18425F",
    "list.activeSelectionBackground": "#D7E8FF",
    "list.activeSelectionForeground": "#001632",
    "list.focusBackground": "#FFF1C2",
    "list.focusOutline": "#0050A4",
    "list.hoverBackground": "#FFE8C2",
    "list.inactiveSelectionBackground": "#EEF4FF",
    "list.inactiveSelectionForeground": "#132949",
    "list.highlightForeground": "#0F8B25",
    "menu.background": "#FFFFFF",
    "menu.foreground": "#051526",
    "menu.selectionBackground": "#FFE38466",
    "menu.selectionForeground": "#001632",
    "menu.separatorBackground": "#0F3A5F",
    "panel.background": "#F7FAFF",
    "panel.border": "#0F3A5F",
    "panelTitle.activeForeground": "#001632",
    "panelTitle.inactiveForeground": "#3E4C5E",
    "pickerGroup.border": "#0F3A5F",
    "pickerGroup.foreground": "#001632",
    "quickInput.background": "#FFFFFF",
    "quickInput.foreground": "#051526",
    "quickInputList.focusBackground": "#FFE38466",
    "quickInputList.focusForeground": "#001632",
    "settings.textInputBorder": "#0F3A5F",
    "settings.numberInputBorder": "#0F3A5F",
    "sideBar.background": "#EFF4FF",
    "sideBar.foreground": "#0A1F33",
    "sideBarTitle.foreground": "#001632",
    "sideBarSectionHeader.background": "#D0E3FF",
    "statusBar.background": "#001632",
    "statusBar.foreground": "#FFFFFF",
    "statusBar.border": "#001632",
    "statusBarItem.hoverBackground": "#007944",
    "statusBarItem.remoteBackground": "#4F378B",
    "statusBarItem.remoteForeground": "#FFFFFF",
    "statusBar.debuggingForeground": "#B00020",
    "tab.activeBackground": "#FFFFFF",
    "tab.activeForeground": "#001632",
    "tab.border": "#0F3A5F",
    "tab.inactiveBackground": "#E2ECFF",
    "tab.inactiveForeground": "#374151",
    "tab.unfocusedInactiveForeground": "#4A5568",
    "terminal.background": "#FFFEFA",
    "terminal.foreground": "#051526",
    "terminalCursor.foreground": "#001632",
    "textLink.foreground": "#0050A4",
    "textLink.activeForeground": "#002347",
    "titleBar.activeBackground": "#F2F6FF",
    "titleBar.activeForeground": "#001632",
    "titleBar.inactiveBackground": "#E4EDFF",
    "titleBar.inactiveForeground": "#3C4D63",
    "tree.indentGuidesStroke": "#44618B"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#00677A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage",
        "storage.type",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#8138B6",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Types and Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class",
        "storage.type.cs"
      ],
      "settings": {
        "foreground": "#A1440A",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#0F6C3D"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language.boolean",
        "constant.language.null"
      ],
      "settings": {
        "foreground": "#B13507"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#004F9F"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "meta.definition.variable",
        "support.variable",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#0A1F33"
      }
    },
    {
      "name": "Annotations and Decorators",
      "scope": [
        "storage.type.annotation",
        "entity.name.function.decorator",
        "punctuation.definition.annotation"
      ],
      "settings": {
        "foreground": "#B83280",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#0050A4"
      }
    },
    {
      "name": "Markup Inserted",
      "scope": [
        "markup.inserted",
        "markup.inline.raw string"
      ],
      "settings": {
        "foreground": "#0F8B25"
      }
    },
    {
      "name": "Markup Deleted",
      "scope": [
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#B00020"
      }
    },
    {
      "name": "Markup Changed",
      "scope": [
        "markup.changed",
        "punctuation.definition.list"
      ],
      "settings": {
        "foreground": "#C25A00"
      }
    },
    {
      "name": "Bold",
      "scope": [
        "markup.bold"
      ],
      "settings": {
        "foreground": "#0A1F33",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "markup.italic"
      ],
      "settings": {
        "foreground": "#0A1F33",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Headings",
      "scope": [
        "markup.heading"
      ],
      "settings": {
        "foreground": "#0050A4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#004F9F"
      }
    },
    {
      "name": "Link URLs",
      "scope": [
        "markup.underline.link",
        "string.other.link"
      ],
      "settings": {
        "foreground": "#0050A4",
        "fontStyle": "underline"
      }
    },
    {
      "name": "Invalid",
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#B00020"
      }
    }
  ]
}
