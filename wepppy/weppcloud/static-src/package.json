{
  "name": "weppcloud-static",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "NODE_ENV=production node scripts/build.mjs",
    "build:dev": "NODE_ENV=development node scripts/build.mjs",
    "clean": "rimraf dist",
    "lint": "eslint --config .eslintrc.cjs --resolve-plugins-relative-to . \"../controllers_js/**/*.js\"",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.mjs",
    "check": "npm run lint && npm run test",
    "smoke": "playwright test --config=playwright.config.mjs"
  },
  "dependencies": {
    "bootstrap": "4.5.3",
    "datatables.net": "1.13.11",
    "datatables.net-bs4": "1.13.11",
    "jquery": "3.5.1",
    "leaflet": "1.7.1",
    "leaflet-ajax": "2.1.0",
    "purecss": "^3.0.0",
    "spin.js": "4.1.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "fs-extra": "11.2.0",
    "globby": "14.0.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.3.3",
    "rimraf": "5.0.5",
    "@playwright/test": "^1.48.2"
  }
}
